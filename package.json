{
  "name": "superui-mono",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pheralb/superui.git"
  },
  "workspaces": [
    "*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:ui": "turbo run build --filter=superui",
    "dev": "turbo run dev --parallel",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "pre-commit": "npm run lint && npm run format",
    "publish": "cd packages/ui && npm publish --access=public"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "tslib": "2.4.0",
    "husky": "8.0.1",
    "nagit": "0.0.18",
    "next-transpile-modules": "9.0.0",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.0"
}
