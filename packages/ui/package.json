{
  "name": "superui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pheralb/superui.git"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint *.ts*",
    "build": "tsup ./index.tsx --format esm,cjs --external react --sourcemap --dts",
    "dev": "tsup ./index.tsx --format esm,cjs --watch --external react",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "postinstall": "node ./postinstall.mjs"
  },
  "devDependencies": {
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-transition-group": "4.4.5",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "react": "18.2.0",
    "tsconfig": "*",
    "tsup": "6.2.2",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@heroicons/react": "1.0.6",
    "@react-aria/overlays": "3.10.0",
    "react-aria": "3.18.0",
    "react-transition-group": "4.4.5",
    "tailwind-merge": "1.5.1"
  }
}
